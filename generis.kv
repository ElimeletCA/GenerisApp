<ItemDrawer>:
    theme_text_color: "Primary"
    on_release:
        self.parent.set_color_item(self)
        app.call_drawerlist_menuderecho(self)
    IconLeftWidget:
        id: icon
        icon: root.icon
        theme_text_color: "Primary"
        text_color: 0, 0, 1, 1


<InformacionScreen>:
    Image:
        source: str(app.ruta("images/fondoinformacion.png"))
        allow_stretch: True
        keep_ratio: True
        size_hint_y: None
        size_hint_x: None
        width: self.parent.width
        height: self.parent.width/self.image_ratio
<Dis_AuditivaScreen>:
    BoxLayout:
        orientation: "vertical"
        padding:20
        spacing:10
        
        canvas:
            Color:
                #Beige_#f5f5dc
                rgb: 0xf5 / 255.0, 0xf5 / 255.0, 0xdc / 255.0
            Rectangle:
                size: self.size
                pos: self.pos
       
        MyCard:
            orientation: 'vertical'
            size_hint: 1, 1
            canvas.before:
                Color:
                    rgba: app.theme_cls.primary_color
                RoundedRectangle:
                    radius: [10]
                    size: self.size
                    pos: self.pos
            FloatLayout:
                size: self.size
                pos: self.pos
                pos_hint: {"x": 1, "y": -0.6}
                BoxLayout:
                    orientation: "vertical"
                    padding:20
                    ScrollView:
                        id: scroll
                        CodeInput:
                            foreground_color: 1,1,1,1
                            background_color: 1,1,1,0
                            allow_copy: False
                            readonly: True
                            size_hint: 1, None
                            text: str(app.showtext("text/discapacidad_auditiva.txt"))
                            height: max(self.minimum_height, scroll.height)
