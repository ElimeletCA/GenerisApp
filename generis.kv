ScreenManager:
    id: screen_manager_rt
    AppScreen:
        #Identificador del screen
        id: appscreen 
        name: 'AppScreen' 
        #Define que manager se encargara de gestionar el screen
        manager: 'screen_manager_rt'
    ContactoScreen:
        #Identificador del screen
        id: contacto_screen 
        name: 'ContactoScreen' 
        #Define que manager se encargara de gestionar el screen
        manager: 'screen_manager_rt'
    ConfiguracionScreen:
        #Identificador del screen
        id: configuracion_screen 
        name: 'ConfiguracionScreen' 
        #Define que manager se encargara de gestionar el screen
        manager: 'screen_manager_rt'
    AcercaDeScreen:
        #Identificador del screen
        id: acercade_screen 
        name: 'AcercaDeScreen' 
