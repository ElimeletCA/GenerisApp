<AppScreen>:

    MDNavigationLayout:
        id: navigation_layout
        ScreenManager:
            id: screen_manager_principal
            InicioScreen:
                #Identificador del screen
                id: inicio_screen 
                name: 'InicioScreen' 
                #Define que manager se encargara de gestionar el screen
                manager: 'screen_manager_principal'
                
        MDNavigationDrawer:
            id: nav_drawer
            ContentNavigationDrawer:
                orientation: "vertical"
                padding: "8dp"
                spacing: "8dp"

                AnchorLayout:
                    anchor_x: "left"
                    size_hint_y: None
                    height: avatar.height

                    Image:
                        id: avatar
                        size_hint: None, None
                        size: "56dp", "56dp"
                        source: str(app.ruta("images/logoeg.jpg"))

                MDLabel:
                    text: "Egregie Mentes"
                    font_style: "Button"
                    size_hint_y: None
                    height: self.texture_size[1]

                MDLabel:
                    text: "support.egregiementes@gmail.com"
                    font_style: "Caption"
                    size_hint_y: None
                    height: self.texture_size[1]

                ScrollView:

                    DrawerList:
                        id: md_list

<InicioScreen>:
    BoxLayout:
        id: box_inicio
        orientation: 'vertical'
        MDToolbar:
            title: "Generis"
            elevation: 10
            left_action_items: [['menu', lambda x: app.root.ids.appscreen.ids.nav_drawer.set_state("toggle")]]
        MDBottomNavigation:
            id: bottom_inicio
            panel_color: get_color_from_hex("#eeeaea")
            selected_color_background: get_color_from_hex("#97ecf8")
            text_color_active: 0, 0, 0, 1
            MDBottomNavigationItem:
                id: navigation_item_informacion
                name: "informacion"
                text: 'Información'
                icon: 'information'
                badge_icon: "numeric-10"
                ScreenManager:
                    id: screen_manager_informacion
                    InformacionScreen:
                        id: informacionscreen
                        name: 'InformacionScreen'
                        manager: 'screen_manager_informacion'
                    Dis_AuditivaScreen:
                        id: dis_auditivascreen
                        name: 'Dis_AuditivaScreen'
                        manager: 'screen_manager_informacion'
                    Dis_ComunicativaScreen:
                        id: dis_comunicativa_screen
                        name: 'Dis_ComunicativaScreen'
                        manager: 'screen_manager_informacion'
                    Dis_FisicaScreen:
                        id: dis_fisica_screen
                        name: 'Dis_FisicaScreen'
                        manager: 'screen_manager_informacion'
                    Dis_VisualScreen:
                        id: dis_visual_screen
                        name: 'Dis_VisualScreen'
                        manager: 'screen_manager_informacion'
                MDFloatingActionButtonSpeedDial:
                    theme_text_color: "Custom"
                    text_color: get_color_from_hex("#001975")
                    md_bg_color: get_color_from_hex("#D40078")
                    data: 
                        {'Discapacidades': 'book-information-variant',
                        'Discapacidad Auditiva': 'ear-hearing',
                        'Discapacidad Comunicativa': 'account-question',
                        'Discapacidad Fisica': 'wheelchair-accessibility',
                        'Discapacidad Visual': 'eye'}
                    callback: app.call_btnspeeddial_informacion
                    root_button_anim: True
                MDBoxLayout:
                    orientation: 'vertical'
                    #define en pixeles el espacio entre los widgets hijo y el borde del widget boxlayout 
                    #define en pixeles el espacio entre los widgets hijo dentro del widget boxlayout 
                    spacing:10
                    #adaptive_height: True
#                    AnchorLayout:
#                        id: asistente
#                        anchor_x: "left"
#                        anchor_y: "bottom"
#                        MDIconButton:
#                            id: asistente
#                            icon: "account-tie-voice"
#                            user_font_size: "64sp"
#                            on_press:
#                                #instrucciones if para verificar si el boton no esta presionado, en tal caso el asistente estara activado, se divido en dos instrucciones para evitar problemas propios de kivy
#                                if app.estado_asistente == 1: app.estado_asistente = 0
#                                else: app.estado_asistente = 1
#                                if app.estado_asistente == 1: self.icon = str("account-tie-voice")
#                                else: self.icon = str("account-tie-voice-off")
#                                print(app.estado_asistente)
            MDBottomNavigationItem:
                id: navigation_item_ayuda
                name: "ayuda"   
                text: 'Ayuda'
                icon: 'help-circle'
                badge_icon: "numeric-10"
                ScreenManager:
                    id: screen_manager_ayuda
                    AyudaScreen:
                        id: ayuda_screen
                        name: 'AyudaScreen'
                        manager: 'screen_manager_ayuda'
                    Pais_CostaRicaScreen:
                        id: pais_costa_rica
                        name: 'Pais_CostaRicaScreen'
                        manager: 'screen_manager_ayuda'
                    Pais_GuatemalaScreen:
                        id: pais_guatemala
                        name: 'Pais_GuatemalaScreen'
                        manager: 'screen_manager_ayuda'
                    Pais_PanamaScreen:
                        id: pais_panama
                        name: 'Pais_PanamaScreen'
                        manager: 'screen_manager_ayuda'
                    Pais_RepublicaDominicanaScreen:
                        id: pais_republica_dominicana
                        name: 'Pais_RepublicaDominicanaScreen'
                        manager: 'screen_manager_ayuda'
                MDFloatingActionButtonSpeedDial:
                    data: 
                        {'Ayuda': 'account-question',
                        'Costa Rica': str(app.ruta("images/CR.png")),
                        'Guatemala': str(app.ruta("images/GT.png")),
                        'Panamá': str(app.ruta("images/PA.png")),
                        'República Dominicana': str(app.ruta("images/DO.png"))}
                    callback: app.call_btnspeeddial_ayuda
                    root_button_anim: True                          
            MDBottomNavigationItem:
                id: navigation_item_comunidad
                name: "comunidad"   
                text: 'Comunidad'
                icon: 'account-group'
                badge_icon: "numeric-10"
            MDBottomNavigationItem:
                id: navigation_item_lugares
